    "x=(3,0)\n",
    "y=(0,4)\n",
    "import numpy as np\n",
    "def distance(x,y):\n",
    "    dis=np.sqrt((y[0]-x[0])**2+(y[1]-x[1])**2)\n",
    "    return dis\n",
